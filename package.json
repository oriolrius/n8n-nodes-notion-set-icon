{
  "name": "n8n-nodes-notion-set-icon",
  "version": "1.0.4",
  "description": "n8n node for setting custom icons on Notion pages",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/oriolrius/n8n-nodes-notion-set-icon#readme",
  "author": {
    "name": "Oriol Rius",
    "email": "oriol@joor.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oriolrius/n8n-nodes-notion-set-icon.git"
  },
  "bugs": {
    "url": "https://github.com/oriolrius/n8n-nodes-notion-set-icon/issues"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@10.12.3",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --fix",
    "prepublishOnly": "pnpm build && pnpm lint -o .eslintcache",
    "release": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags",
    "test:n8n": "./start-n8n.sh",
    "start": "./start-n8n.sh",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:notion": "playwright test tests/e2e/notion",
    "test:notion:list": "playwright test tests/e2e/notion --list",
    "test:docker": "cd tests/e2e/docker && make test",
    "test:docker:setup": "cd tests/e2e/docker && make setup",
    "test:docker:validate": "cd tests/e2e/docker && make validate",
    "test:docker:ui": "cd tests/e2e/docker && make test-ui",
    "test:docker:clean": "cd tests/e2e/docker && make clean",
    "test:docker:n8n-setup": "cd tests/e2e/docker && make setup-n8n",
    "test:integration": "npm run test:docker"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NotionSetIconApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/NotionSetIcon/NotionSetIcon.node.js"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/node": "^20.14.8",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.56.0",
    "playwright": "^1.55.1",
    "prettier": "^3.3.2",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "sqlite3": "^5.1.7"
  }
}
